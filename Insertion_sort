//  Insertion sort

#include<iostream>
#define MAX 100
using namespace std;

void Insertion_sort(int arr[], int n)
{
    int i,t,pos;
    for (int i=0 ; i<n; i++)
    {
        t=arr[i];
        pos=i;
        while(pos>0 && arr[pos-1]>t)
        {
            arr[pos]=arr[pos-1];
            pos--;
        }
        arr[pos]=t;
    }
}
int main()
{
    
    return 0;
}

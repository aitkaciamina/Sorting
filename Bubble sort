// Bubble sorting

#include<iostream>
#define MAX 100
using namespace std;

int Bubble_sorting(int arr[], int n)
{
    int tmp;
    for(int i=0 ;i<n; i++)
    {
        for(int j=i+1; j<n; j++)
        {
            if (arr[i]>arr[j])
            {
                tmp=arr[i];
                arr[i]=arr[j];
                arr[j]=tmp;
            }
        }
    }
}
int main()
{
    
    return 0;
}
